{
  "name": "vue-md-loader",
  "version": "1.1.9",
  "description": "Webpack loader for converting Markdown files to ALIVE Vue components.",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./test/coverage/"
  },
  "scripts": {
    "dev:example": "cd example && cross-env NODE_ENV=development webpack-dev-server --hot --inline --port 8888",
    "build:example": "cd example && cross-env NODE_ENV=production webpack",
    "lint": "eslint --ext .js,.vue --config .eslintrc.js index.js src example",
    "test": "nyc mocha",
    "coveralls": "cat test/coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxsms/vue-md-loader.git"
  },
  "keywords": [
    "vue",
    "markdown",
    "md",
    "webpack",
    "loader",
    "live",
    "demo",
    "document"
  ],
  "author": "wxsm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wxsms/vue-md-loader/issues"
  },
  "homepage": "https://github.com/wxsms/vue-md-loader#readme",
  "devDependencies": {
    "@babel/preset-env": "7.14.5",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "chai": "4.3.4",
    "cheerio": "1.0.0-rc.10",
    "coveralls": "3.1.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.6",
    "eslint": "7.29.0",
    "eslint-config-standard": "16.0.3",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "7.11.1",
    "markdown-it-anchor": "8.0.1",
    "mocha": "9.0.1",
    "nyc": "15.1.0",
    "style-loader": "2.0.0",
    "vue": "2.6.14",
    "vue-loader": "15.9.7",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.46.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "highlight.js": "^11.0.0",
    "loader-utils": "^2.0.0",
    "markdown-it": "^12.0.0"
  }
}
