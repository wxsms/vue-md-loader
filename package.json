{
  "name": "vue-md-loader",
  "version": "2.0.0",
  "description": "Webpack loader for converting Markdown files to ALIVE Vue components.",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./test/coverage/"
  },
  "scripts": {
    "dev:example": "cd example && cross-env NODE_ENV=development webpack-dev-server --hot --port 8888 --static .",
    "build:example": "cd example && cross-env NODE_ENV=production webpack",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc mocha",
    "coveralls": "cat test/coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxsms/vue-md-loader.git"
  },
  "keywords": [
    "vue",
    "markdown",
    "md",
    "webpack",
    "loader",
    "live",
    "demo",
    "document"
  ],
  "author": "wxsm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wxsms/vue-md-loader/issues"
  },
  "homepage": "https://github.com/wxsms/vue-md-loader#readme",
  "devDependencies": {
    "@babel/preset-env": "7.15.8",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "cheerio": "1.0.0-rc.10",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "css-loader": "6.4.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-vue": "7.20.0",
    "markdown-it-anchor": "8.4.1",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "prettier": "2.4.1",
    "style-loader": "3.3.1",
    "vue": "2.6.14",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "2.6.14",
    "webpack": "5.60.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1"
  },
  "dependencies": {
    "highlight.js": "^11.0.0",
    "loader-utils": "^3.0.0",
    "markdown-it": "^12.0.0"
  }
}
